<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.test.hb">
	<class name="Employee" table="employee">
		<id name="id" column="emp_id" type="string" unsaved-value="null">
			<generator class="assigned"/>
		</id>
		<property name="emp_name" type="string" length="100"/>
		<property name="salary" type="integer"/>
		<property name="office" type="string"/>
	</class>
</hibernate-mapping>